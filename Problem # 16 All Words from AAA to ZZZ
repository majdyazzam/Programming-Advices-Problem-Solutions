#include <iostream>
using namespace std;

int readNnumber() {
    int num = 0;
   
    do {
cout << "Please Enter Positive Number : " << endl;
        cin >> num;
    } while (num < 0);
        
    return num;
}
void printAllWordsFromAAAtoZZZ() {
    cout << "\n";
    string word;
    for (int i = 65; i < 91; i++) {
        for (int j = i; j < 91; j++) {
            for (int k = i; k < 91; k++) {
                word += char(i);
                word += char(j);
                word += char(k);

                cout << word << endl;
                word = "";
            }
            
        }
        
    }
    
}
int main()
{
    printAllWordsFromAAAtoZZZ();
   
}

